        <InArgument x:TypeArguments="x:String">
          <CSharpValue x:TypeArguments="x:String" sap2010:WorkflowViewState.IdRef="CSharpValue`1_19" xml:space="preserve">@"SELECT TOP 50 *,
       CASE 
           WHEN LEN(REPLACE(REPLACE(REPLACE(IDENTIFICACION, '-', ''), '.', ''), ' ', '')) = 9 THEN 
               RIGHT(CONCAT('0000000000', REPLACE(REPLACE(REPLACE(IDENTIFICACION, '-', ''), '.', ''), ' ', '')), 10)
           WHEN LEN(REPLACE(REPLACE(REPLACE(IDENTIFICACION, '-', ''), '.', ''), ' ', '')) = 12 THEN 
               RIGHT(CONCAT('0000000000000', REPLACE(REPLACE(REPLACE(IDENTIFICACION, '-', ''), '.', ''), ' ', '')), 13)
           ELSE 
               REPLACE(REPLACE(REPLACE(IDENTIFICACION, '-', ''), '.', ''), ' ', '')
       END AS [IDENTIFICACION_CURADA]
FROM CLIENTES_CITACIONES 
WHERE IDENTIFICACION NOT IN ('NO DISPONIBLE', 'NO VISIBLE')"</CSharpValue>
        </InArgument>
      </ui:ExecuteQuery.Sql>
